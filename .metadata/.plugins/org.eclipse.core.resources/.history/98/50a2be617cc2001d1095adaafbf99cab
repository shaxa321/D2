

public class Main {
	
	private void stampaRettangoli(Rettangolo ret1,Rettangolo ret2) {
		ret1.stampaRettangolo();
		ret2.stampaRettangolo();
	}

	
	
	public static void main(String[] args) {
         Rettangolo rettangolo1 = new Rettangolo(40f,50f);
         Rettangolo rettangolo2=new Rettangolo(30f,20f);
         rettangolo1.stampaRettangolo();
         rettangolo2.stampaRettangolo();
         stampaRettangoli(rettangolo1,rettangolo2);
         
    }
}
